{% extends "layouts/base.njk" %} 
{% block content %}
  <article>
    <h1>{{ title }}</h1>
    {{ content | safe }}
    <ul>
      {% for image in collections.cloudinaryGallery %}
        <li>
          <img src="{{ image.secure_url }}" alt="{{ image.public_id }}">
        </li>
      {% endfor %}
    </ul>    
  </article>
{% endblock %}