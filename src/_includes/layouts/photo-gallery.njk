{% extends "layouts/base.njk" %} 
{% block content %}
  <h1>{{ title }}</h1>
  {{ content | safe }}
  <ul>
    {% for photo in collections.cloudinaryGallery %}
      <li>
        <a href="photos/{{ photo.public_id }}">
          <img src="{{ photo.secure_url }}" alt="{{ photo.public_id }}">
        </a>
      </li>
    {% endfor %}
  </ul>    
{% endblock %}